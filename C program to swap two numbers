// C program to swap two numbers
#include<stdio.h>
int main(){
    int a,b;
    printf("Enter the value of a\n");
    scanf("%d",&a);
    printf("Enter the value of b\n");
    scanf("%d",&b);
    
    
    int temp = a;         // swaping the value 
    a = b;
    b = temp;
    
    printf("After swaping the number is : %d and %d ", a,b);
    return 0;
    
}
